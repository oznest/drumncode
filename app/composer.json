{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^4.3.2",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "eightpoints/guzzle-bundle": ">=8.5.2",
        "friendsofsymfony/elastica-bundle": "^6.6",
        "hautelook/alice-bundle": "^2.15.1",
        "jms/serializer-bundle": "^5.5.1",
        "lexik/jwt-authentication-bundle": "^3.1.1",
        "nelmio/api-doc-bundle": "^5.5",
        "open-telemetry/exporter-otlp": "^1.3.2",
        "open-telemetry/sdk": "^1.7",
        "php-http/guzzle7-adapter": "^1.1",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.2",
        "promphp/prometheus_client_php": "^2.14.1",
        "symfony/amqp-messenger": "^7.3.2",
        "symfony/asset": "^7.3",
        "symfony/asset-mapper": "^7.3.2",
        "symfony/browser-kit": "^7.3.2",
        "symfony/config": "^7.3.2",
        "symfony/console": "^7.3.2",
        "symfony/doctrine-messenger": "^7.3.2",
        "symfony/dotenv": "^7.3.2",
        "symfony/expression-language": "^7.3.2",
        "symfony/flex": "^2.8.1",
        "symfony/form": "^7.3.2",
        "symfony/framework-bundle": "^7.3.2",
        "symfony/http-client": "^7.3.2",
        "symfony/intl": "^7.3.2",
        "symfony/mailer": "^7.3.2",
        "symfony/mime": "^7.3.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.3",
        "symfony/process": "^7.3",
        "symfony/property-access": "^7.3.2",
        "symfony/property-info": "^7.3.1",
        "symfony/runtime": "^7.3.1",
        "symfony/security-bundle": "^7.3.2",
        "symfony/serializer": "^7.3.2",
        "symfony/stimulus-bundle": "^2.29.2",
        "symfony/string": "^7.3.2",
        "symfony/translation": "^7.3.2",
        "symfony/twig-bundle": "^7.3.2",
        "symfony/ux-turbo": "^2.29.2",
        "symfony/validator": "^7.3.2",
        "symfony/var-exporter": "^7.3.2",
        "symfony/web-link": "^7.3",
        "symfony/yaml": "^7.3.2",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/twig": "^2.12|^3.21.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "tbachert/spi": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.3"
        }
    },
    "require-dev": {
        "deptrac/deptrac": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "fakerphp/faker": "^1.24.1",
        "phpunit/phpunit": "^9.6.25",
        "slevomat/coding-standard": "^8.20.0",
        "squizlabs/php_codesniffer": "^3.13.2",
        "symfony/css-selector": "^7.3",
        "symfony/debug-bundle": "^7.3",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3.1",
        "symfony/stopwatch": "^7.3",
        "symfony/web-profiler-bundle": "^7.3.2"
    }
}
